      "cell_type": "code",
      "source": [
        "## Hyperparameters\n",
        "\n",
        "learning_rate = 0.01\n",
        "epochs = 100\n",
        "batch_size = 50"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "metadata": {
        "id": "AfsaGCiBGhu6",
        "colab_type": "code",
